{
  "connection": "default",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal"
    },
    "brands": {
      "collection": "brand",
      "via": "products",
      "dominant": true
    },
    "materials": {
      "collection": "material",
      "via": "products",
      "dominant": true
    },
    "tags": {
      "collection": "tag",
      "via": "products",
      "dominant": true
    },
    "sizes": {
      "collection": "size",
      "via": "products",
      "dominant": true
    },
    "condition": {
      "model": "condition",
      "via": "products"
    },
    "colors": {
      "collection": "color",
      "via": "products"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "user": {
      "model": "user",
      "via": "products",
      "plugin": "users-permissions"
    },
    "categories": {
      "collection": "category",
      "via": "products"
    },
    "city": {
      "model": "city",
      "via": "products"
    },
    "Url": {
      "unique": true,
      "required": true,
      "type": "string"
    }
  }
}