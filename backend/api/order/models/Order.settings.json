{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Reference": {
      "type": "uid"
    },
    "Price": {
      "type": "decimal"
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "order_created",
        "work_order_received",
        "work_started",
        "work_finished",
        "sent_back_to_customer"
      ]
    },
    "Ime": {
      "type": "string"
    },
    "Prezime": {
      "type": "string"
    },
    "Telefon": {
      "type": "string"
    },
    "Email": {
      "type": "string"
    },
    "Adresa": {
      "type": "string"
    },
    "products": {
      "collection": "product"
    }
  }
}